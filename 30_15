#include <stdio.h>
#include<stdlib.h>

int main()
{ 
 int input;
 int i1, i2, i3, i4;
 
 scanf("%d", &input);
 
 i1 = ((input / 1000) + 7) % 10;
 i2 = (((input % 1000) / 100) + 7) % 10;
 i3 = (((input % 100) /10) + 7) % 10;
 i4 = ((input % 10) + 7) % 10;

 
 printf("%d%d%d%d", i3, i4, i1, i2);

return 0;  
} 
