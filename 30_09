#include <stdio.h>
#include<stdlib.h>

int main()
{ 
	int m, m1 = 1;
	int n, n1 = 1;
	int mn, mn1 = 1;
	
	scanf("%d %d", &m, &n);
	mn = m - n;
	
	for(int i=1; i<=m; i++){
		m1 = m1 * i;
	}
	for(int i=1; i<=n; i++){
		n1 = n1 * i;
	}
	for(int i=1; i<=mn; i++){
		mn1 = mn1 * i;
	}
	
	m1 = (m1/n1) / mn1;
	printf("%d", m1);
 
 return 0;  
} 
