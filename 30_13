#include <stdio.h>
#include<stdlib.h>
int main()
{
    char s[80], i=0;
    scanf("%s", s);
    while(s[i] != '\0')
    {
        if(s[i] <= 'z' && s[i] >= 'a')
            s[i] -= 32;
        if(s[i] <= '9' && s[i] >= '0')
            s[i] = '0';
        i++;
    }
    printf("%s",s);
    
	return 0;  
}
